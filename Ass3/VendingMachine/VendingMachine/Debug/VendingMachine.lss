                                 ; the parameter @0 is the memory address for that word
                                 .macro clear 
                                 	ldi YL, low(@0) ; load the memory address to Y 
                                 	ldi YH, high(@0) 
                                 	clr temp 
                                 	st Y+, temp ; clear the two bytes at @0 in SRAM 
                                 	st Y, temp 
                                 .endmacro
